#nginx
CONTAINER_NAME_NGINX        = container name nginx
LOCAL_PORT_NGINX            = local port nginix
CONTAINER_PORT_NGINX        = container port nginx

#Phpfpm
CONTAINER_NAME_PHP_FPM      = container name phpfpm

#MySQL
CONTAINER_NAME_DB           = db
LOCAL_PORT_DB               = local port db
CONTAINER_PORT_DB           = container port db
DB_NAME                     = DB name
DB_USER                     = DB user name
DB_PASSWORD                 = DB password

#Redis
CONTAINER_NAME_REDIS        = container name redis
LOCAL_REDIS_REDIS           = local port redis
CONTAINER_PORT_REDIS        = container port redis
REDIS_PASSWORD              = redis password

#Memcached
CONTAINER_NAME_MEMCACHED    = container name memcached
LOCAL_PORT_MEMCACHED        = local port memcached
CONTAINER_PORT_MEMCACHED    = container port memcached

