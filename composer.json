{
  "name": "project/otus",
  "description": "Yii2",
  "minimum-stability": "dev",
  "keywords": [
    "yii2",
    "framework"
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "license": "BSD-3-Clause",
  "require": {
    "bower-asset/admin-lte": "2.3.8",
    "bower-asset/font-awesome": "^4.0",
    "fuko-php/masked": "^1.1",
    "guzzlehttp/guzzle": "7.8.1",
    "guzzlehttp/psr7": "2.6.2",
    "mikemadisonweb/yii2-rabbitmq": "^2.5",
    "php-amqplib/php-amqplib": "^3.1",
    "ramsey/uuid": "^4.2",
    "trntv/cheatsheet": "^0.1@dev",
    "trntv/yii2-file-kit": "^1.0.0",
    "unclead/yii2-multiple-input": "~2.0",
    "vlucas/phpdotenv": "^2.0",
    "webmozart/assert": "^1.10",
    "yetanotherape/diff-match-patch": "*",
    "yii2tech/csv-grid": "^1.0",
    "yiisoft/yii2": "^2.0.6",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-imagine": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.4",
    "fzaninotto/faker": "^1.5",
    "mockery/mockery": "^1.4",
    "overtrue/phplint": "^4.0",
    "phpunit/phpunit": "^9.5",
    "yiisoft/yii2-debug": "^2.0.0",
    "yiisoft/yii2-gii": "^2.0.0",
    "zircote/swagger-php": "^3.2"
  },
  "replace": {
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "yiisoft/yii2-composer": true
    }
  },
  "scripts": {
    "generate-swagger": "openapi api src --output api/web/swagger/swagger.json --format json"
  },
  "autoload": {
    "files": [
      "common/helpers.php"
    ],
    "psr-4": {
      "app\\": "src/"
    }
  },
  "prefer-stable": true
}
