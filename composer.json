{
    "name": "project/bookstore",
    "description": "Console application for searching books using ElasticSearch",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "elasticsearch/elasticsearch": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
