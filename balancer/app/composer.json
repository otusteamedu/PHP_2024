{
  "name": "vendor/package",
  "description": "Balancer project",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya"
    }
  ],
  "require": {
    "php": "^7.4",
    "monolog/monolog": "^2.0",
    "predis/predis": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
